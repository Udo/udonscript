function roll() {
    return(5)
}

function main() {
    print("Test 1: Direct assignment")
    var x = roll()
    print("x = " + to_string(x))
    
    print("Test 2: Map assignment from literal")
    var scores = {}
    scores["str"] = 5
    print("scores[str] = " + to_string(scores["str"]))
    
    print("Test 3: Map assignment from function call")
    scores["dex"] = roll()
    print("scores[dex] = " + to_string(scores["dex"]))
}
