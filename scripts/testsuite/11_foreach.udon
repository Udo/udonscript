// Test: Foreach loop with key and value
function main() {
	var data = {a: 1, b: 2, c: 3};
	var sum = 0;
	foreach (var key, val in data) {
		sum = sum + val;
	}
	print("sum:", sum);
	
	// Test with single var (values)
	var count = 0;
	foreach (var v in data) {
		count = count + v;
	}
	print("value_sum:", count);
}
