// Demo: FFI via dl_open

function main() {
    print("=== FFI Math Demo ===")
    var ns = dl_open("libm.so.6")
    print("pow(2,5):", ns:call("pow(float,float):float", 2, 5))
    print("sin(1.0):", ns:call("sin(float):float", 1.0))
    print("floor(3.7):", ns:call("floor(float):float", 3.7))
    ns:close()
}
