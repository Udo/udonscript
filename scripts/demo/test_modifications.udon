// Test object/array modification
function main() {
    print("=== Testing Object/Array Modification ===")
    print("")
    
    // Test multi-line object literals
    print("Creating object with newlines:")
    var person = {
        name: "Alice",
        age: 30,
        city: "NYC"
    }
    print("  name:", person:name)
    print("  age:", person:age)
    print("  city:", person:city)
    print("")
    
    // Test property modification
    print("Modifying properties:")
    person:age = 31
    person:city = "Boston"
    print("  age:", person:age)
    print("  city:", person:city)
    print("")
    
    // Test adding new properties
    print("Adding new property:")
    person:active = true
    print("  active:", person:active)
    print("")
    
    // Test bracket index assignment
    print("Using bracket notation:")
    var data = {}
    data[0] = "first"
    data[1] = "second"
    data["key"] = "value"
    print("  data[0]:", data[0])
    print("  data[1]:", data[1])
    print("  data['key']:", data["key"])
    print("")
    
    // Test else-if without braces
    print("Testing else-if without braces:")
    var x = 5
    if (x < 5)
        print("  x < 5")
    else if (x == 5)
        print("  x == 5")
    else
        print("  x > 5")
    print("")
    
    // Test while without braces
    print("Testing while without braces:")
    var i = 0
    while (i < 3)
        i = i + 1
    print("  Final i:", i)
    print("")
    
    print("All tests passed!")
    
    return 0
}
