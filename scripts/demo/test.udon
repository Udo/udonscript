// Test script for UdonScript
function greet(name) {
    print("Hello, " + name + "!")
    return "Greeted " + name
}

function calculate() {
    var sum = 0
    for (var i = 1; i <= 10; i = i + 1) {
        sum = sum + i
    }
    print("Sum of 1 to 10: " + to_string(sum))
    return sum
}

function main() {
    print("=== UdonScript Test ===".to_lower())
    greet("World")
    var result = calculate()
    
    var v = vec3(1.0, 2.0, 3.0)
    print("Vector: " + to_string(v))
    
    return result
}
