function roll_4d6_drop_lowest() {
    var rolls = {}
    print("About to loop")
    for (var i = 0; i < 4; i = i + 1) {
        print("Loop i=" + to_string(i))
        rolls[i] = 1 + floor(rand() * 6)
        print("Set rolls[" + to_string(i) + "] = " + to_string(rolls[i]))
    }
    print("Done with loop")
    return(rolls[0])
}

function main() {
    print("Calling function")
    var result = roll_4d6_drop_lowest()
    print("Result: " + to_string(result))
}
